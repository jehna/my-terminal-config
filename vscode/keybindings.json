// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+shift+q",
    "command": "rewrap.rewrapComment",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+q",
    "command": "-rewrap.rewrapComment",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+a",
    "command": "editor.action.reindentlines"
  }
]