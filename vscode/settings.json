{
  "editor.tabSize": 2,
  "workbench.startupEditor": "newUntitledFile",
  "editor.renderWhitespace": "all",
  "files.trimTrailingWhitespace": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.suggestSelection": "first",
  "org.todoKeywords": ["TODO", "DOING", "DONE", "BLOCKED"],
  "extensions.ignoreRecommendations": false,
  "calva.prettyPrintingOptions": {
    "enabled": false,
    "printEngine": "pprint",
    "width": 120
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[markdown]": {
    "editor.defaultFormatter": "fcrespo82.markdown-table-formatter"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.scrollback": 10000,
  "[clojure]": {
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.autoClosingQuotes": "beforeWhitespace",
    "editor.formatOnType": true,
    "editor.autoIndent": "advanced",
    "editor.formatOnPaste": true,
    "files.trimTrailingWhitespace": false,
    "editor.matchBrackets": "always",
    "editor.renderIndentGuides": false,
    "editor.parameterHints.enabled": false,
    "editor.guides.indentation": false
  },
  "calva.paredit.defaultKeyMap": "none",
  "calva.evalOnSave": true,
  "workbench.editorAssociations": {
    "git-rebase-todo": "default",
    "*.ipynb": "jupyter-notebook"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[sql]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/output.calva-repl": true
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "debug.allowBreakpointsEverywhere": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "COMMIT_EDITMSG": "git-commit"
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "COBOL": true,
    "COBOLIT": true,
    "ACUCOBOL": true,
    "COBOL_MF_LISTFILE": false,
    "COBOL_PCOB_LISTFILE": false,
    "COBOL_ACU_LISTFILE": false,
    "COBOL_MF_PREP": false,
    "mfu": false,
    "utreport": false,
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": true
  },
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    }
  },
  "editor.guides.indentation": false,
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": false,
  "debug.javascript.autoAttachFilter": "disabled",
  "codeQL.telemetry.enableTelemetry": true,
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.inheritEnv": false,
  "jupyter.askForKernelRestart": false,
  "hexeditor.columnWidth": 16,
  "hexeditor.showDecodedText": true,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "slint.preview.style": "cupertino-dark",
  "dart.openDevTools": "flutter"
}
